package
{
	import fl.controls.listClasses.ICellRenderer;
	import fl.data.DataProvider;

	public interface IListSimple
	{
		function set dataProvider(dp:DataProvider):void;
		function get dataProvider():DataProvider;
		function set renderer(value:ICellRenderer):void;
		function get renderer():ICellRenderer;
		function set rowHeight(value:Number);
		function get rowHeight():Number;
		function set rowCount(value:uint);
		function get rowCount():uint;
		function getRendererOfData(data:Object);
	}
}