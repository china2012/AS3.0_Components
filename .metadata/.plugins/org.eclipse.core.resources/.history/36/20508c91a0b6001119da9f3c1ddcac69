package
{
	import fl.data.DataProvider;

	public interface IListSimple
	{
		function set dataProvider(dp:DataProvider);
		function get dataProvider():DataProvider;
		function set renderer();
		function get renderer();
		function set rowHeight();
		function get rowHeight();
		function set rowCount();
		function get rowCount();
		function getRendererOfData(data:Object);
	}
}